# Enabling Browser Caching
<IfModule mod_expires.c>

  ExpiresActive on
  ExpiresDefault                                    "access plus 5 months"
  
  # DATA
  ExpiresByType text/html                           "access plus 0 seconds"
  ExpiresByType text/xml                            "access plus 0 seconds"
  ExpiresByType text/plain                          "access plus 0 seconds"
  ExpiresByType application/xml                     "access plus 0 seconds"
  ExpiresByType application/json                    "access plus 0 seconds"
  ExpiresByType application/ld+json                 "access plus 0 seconds"
  ExpiresByType application/vnd.geo+json            "access plus 0 seconds"
  ExpiresByType application/rss+xml                 "access plus 1 hour"
  ExpiresByType application/atom+xml                "access plus 1 hour"
  ExpiresByType text/x-component                    "access plus 1 hour"
  
  # SCRIPTS
  ExpiresByType text/css                            "access plus 5 months" 
  ExpiresByType text/javascript                     "access plus 5 months" 
  ExpiresByType application/javascript              "access plus 5 months" 
  ExpiresByType application/x-javascript            "access plus 5 months" 
  
  # IMAGES
  ExpiresByType image/gif                           "access plus 5 months"
  ExpiresByType image/png                           "access plus 5 months"
  ExpiresByType image/jpe                           "access plus 5 months"
  ExpiresByType image/jpg                           "access plus 5 months"
  ExpiresByType image/jpeg                          "access plus 5 months"
  ExpiresByType image/jp2                           "access plus 5 months"
  ExpiresByType image/pipeg                         "access plus 5 months"
  ExpiresByType image/bmp                           "access plus 5 months"
  ExpiresByType image/tiff                          "access plus 5 months"
  ExpiresByType image/webp                          "access plus 5 months"
  ExpiresByType image/avif                          "access plus 5 months"
  ExpiresByType image/svg+xml                       "access plus 5 months"
  ExpiresByType image/vnd.microsoft.icon            "access plus 5 months"
  
  # ICONS
  ExpiresByType image/ico                           "access plus 5 months"
  ExpiresByType image/icon                          "access plus 5 months"
  ExpiresByType text/ico                            "access plus 5 months"
  ExpiresByType image/x-ico                         "access plus 5 months"
  ExpiresByType image/x-icon                        "access plus 5 months"
  ExpiresByType application/ico                     "access plus 5 months"
  
  # AUDIO
  ExpiresByType audio/ogg                           "access plus 5 months"
  ExpiresByType audio/basic                         "access plus 5 months"
  ExpiresByType audio/mid                           "access plus 5 months"
  ExpiresByType audio/midi                          "access plus 5 months"
  ExpiresByType audio/mpeg                          "access plus 5 months"
  ExpiresByType audio/x-aiff                        "access plus 5 months"
  ExpiresByType audio/x-mpegurl                     "access plus 5 months"
  ExpiresByType audio/x-pn-realaudio                "access plus 5 months"
  ExpiresByType audio/x-wav                         "access plus 5 months"
  
  # VIDEO
  ExpiresByType video/ogg                           "access plus 5 months"
  ExpiresByType video/mp4                           "access plus 5 months"
  ExpiresByType video/webm                          "access plus 5 months"
  ExpiresByType video/x-msvideo                     "access plus 5 months"
  ExpiresByType video/mpeg                          "access plus 5 months"
  ExpiresByType video/quicktime                     "access plus 5 months"
  ExpiresByType video/x-la-asf                      "access plus 5 months"
  ExpiresByType video/x-ms-asf                      "access plus 5 months"
  ExpiresByType x-world/x-vrml                      "access plus 5 months"
  
  # FONTS
  ExpiresByType font/truetype                       "access plus 5 months"
  ExpiresByType font/opentype                       "access plus 5 months"
  ExpiresByType application/x-font-ttf              "access plus 5 months"
  ExpiresByType application/x-font-woff             "access plus 5 months"
  ExpiresByType application/font-woff               "access plus 5 months"
  ExpiresByType application/font-woff2              "access plus 5 months"
  ExpiresByType application/vnd.ms-fontobject       "access plus 5 months"
  
  # FLASH
  ExpiresByType application/x-shockwave-flash       "access plus 5 months"
  ExpiresByType video/x-flv                         "access plus 5 months"
  
  # MANIFEST
  ExpiresByType application/x-web-app-manifest+json "access plus 0 seconds"
  ExpiresByType text/cache-manifest                 "access plus 0 seconds"
  
  # OTHERS
  ExpiresByType application/pdf                     "access plus 5 months"
  ExpiresByType image/vnd.wap.wbmp                  "access plus 5 months"
  ExpiresByType application/vnd.wap.wbxml           "access plus 5 months"
  ExpiresByType application/smil                    "access plus 5 months"
  
  <IfModule mod_headers.c>
    Header append Cache-Control "public"
    <FilesMatch "\.(ico|flv|jpg|jpeg|png|gif|css|swf)$">
      Header set Cache-Control "max-age=2578400, public"
    </FilesMatch>
    <FilesMatch "\.(html|htm)$">
      Header set Cache-Control "max-age=7200, private, must-revalidate"
    </FilesMatch>
    <FilesMatch "\.(pdf)$">
      Header set Cache-Control "max-age=85400, public"
    </FilesMatch>
    <FilesMatch "\.(js)$">
      Header set Cache-Control "max-age=2578400, private"
    </FilesMatch>
  </IfModule>

</IfModule>